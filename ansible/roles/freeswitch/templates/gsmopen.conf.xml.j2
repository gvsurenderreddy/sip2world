<configuration name="gsmopen.conf" description="GSMopen Configuration">
  <global_settings>
    <param name="debug" value="0"/>
    <param name="dialplan" value="XML"/>
    <param name="context" value="{{ sip_domain }}"/>
    <param name="hold-music" value="$${moh_uri}"/>
    <param name="destination" value="100"/>
  </global_settings>
  <per_interface_settings>
    <interface id="1" name="mts">
      <param name="controldevice_name" value="/dev/ttyUSB1"/>
      <param name="controldevice_audio_name" value="/dev/ttyUSB0"/>
      <param name="capture_boost" value="30"/>
      <param name="playback_boost" value="-10"/>
    </interface>
  </per_interface_settings>
</configuration>
